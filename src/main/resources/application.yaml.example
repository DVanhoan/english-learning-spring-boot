server:
  port: 8080
  servlet:
    context-path: /e-learning

spring:
  datasource:
    url: jdbc:mysql://localhost:3306/ELearning?serverTimezone=UTC
    username: main
    password: 1234
    driver-class-name: com.mysql.cj.jdbc.Driver
  jpa:
    database-platform: org.hibernate.dialect.MySQLDialect
    database: mysql
